# AWS S3 Configuration
# Copy this file to .env and fill in your actual AWS credentials

# AWS Region (e.g., us-east-1, us-east-2, ap-south-1, etc.)
AMZ_REGION=us-east-2

# AWS Access Key ID
AMZ_ACCESS_KEY_ID=your_access_key_here

# AWS Secret Access Key
AMZ_SECRET_ACCESS_KEY=your_secret_key_here

# AWS S3 Bucket Name
AMZ_BUCKET_NAME=your_bucket_name_here
